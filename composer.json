{
    "name": "discite/discite-php",
    "description": "A PHP library for managing MySQL databases in an object-oriented way.",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "DisciteDB\\": "src/DisciteDB/"
      }
    },
    "authors": [
      {
        "name": "Romain QUINTAINE",
        "email": "quintainer@icloud.com"
      }
    ],
    "require": {
